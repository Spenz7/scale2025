{
  "_version": "1.65.0",
  "sap.app": {
    "id": "project1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "SkyLink Engagement Hub",
    "description": "Passenger Retention Tool",
    "resources": "resources.json",
    "dataSources": {
      "engagementService": {
        "uri": "http://localhost:4005/engagement/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "tablet": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.108.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project1.i18n.i18n"
        }
      },
      "engagement": {
        "type": "sap.ui.model.odata.v4.ODataModel",
        "dataSource": "engagementService",
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "controlId": "app",
        "controlAggregation": "pages"
      },
      "routes": [
        {
          "pattern": "",
          "name": "engagement",
          "target": "engagementView"
        }
      ],
      "targets": {
        "engagementView": {
          "viewName": "Engagement",
          "viewLevel": 1,
          "viewId": "engagementView"
        }
      }
    }
  }
}